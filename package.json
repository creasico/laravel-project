{
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@8.7.0",
  "author": "Creasi Developers <developers@creasi.co>",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .ts,.json,.vue resources",
    "prepare": "husky install scripts/husky",
    "release": "standard-version -s"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@creasico/eslint-config": "^0.0.1",
    "@fontsource/nunito": "^5.0.13",
    "@iconify/vue": "^4.1.1",
    "@inertiajs/core": "^1.0.12",
    "@inertiajs/vue3": "^1.0.12",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@sentry/vite-plugin": "^2.8.0",
    "@sentry/vue": "^7.73.0",
    "@types/node": "^18.18.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/runtime-core": "^3.3.4",
    "@vueuse/core": "^10.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "laravel-vite-plugin": "^0.8.1",
    "lint-staged": "^15.0.2",
    "naive-ui": "^2.35.0",
    "standard-version": "^9.5.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "~4.4.11",
    "vite-plugin-pwa": "^0.16.5",
    "vite-plugin-windicss": "^1.9.1",
    "windicss": "^3.5.6",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-window": "^7.0.0",
    "ziggy-js": "^1.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{json,js,ts,vue}": [
      "eslint --fix"
    ],
    "{app,config,database,resources,tests}/**/*.php": [
      "php vendor/bin/pint --preset laravel"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": "@creasico/eslint-config/vue",
    "parserOptions": {
      "tsconfigRootDir": ".",
      "project": "./tsconfig.json"
    }
  },
  "standard-version": {}
}
